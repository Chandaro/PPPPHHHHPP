{
	"version": 2,
	"builds": [
		{ "src": "index.php", "use": "@vercel/php" },
		{ "src": "displayData.php", "use": "@vercel/php" },
		{ "src": "firebase.php", "use": "@vercel/php" }
	],
	"routes": [
		{ "src": "/index.php", "dest": "/index.php" },
		{ "src": "/displayData.php", "dest": "/displayData.php" },
		{ "src": "/firebase.php", "dest": "/firebase.php" },
		{ "src": "/(.*)", "dest": "/index.php" }
	]
}